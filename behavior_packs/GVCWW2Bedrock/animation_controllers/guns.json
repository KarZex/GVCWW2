{
  "format_version": "1.10.0",
  "animation_controllers": {
    "controller.animation.guns": {
      "states": {
        "default": {
          "on_entry": [
            "/tag @s remove subattack"
          ],
          "transitions": [
            {
              "tank": "query.is_riding && (variable.attack_time > 0.0)"
            },
            {
              "tank": "query.is_riding && query.is_item_name_any('slot.weapon.mainhand', 0, 'gun:tank') && query.is_using_item"
            },
            {
              "mgg_1": "query.is_riding && query.is_item_name_any('slot.weapon.mainhand', 0, 'gun:mgg') && query.is_using_item"
            },
            {
              "mgj_1": "query.is_riding && query.is_jumping && query.get_equipped_item_name != 'mgg'"
            },
            {
              "shield": "query.is_sneaking && query.is_item_name_any('slot.weapon.offhand', 0, 'minecraft:shield')"
            },
            {
              "fire": "query.is_item_name_any('slot.weapon.mainhand', 0, 'gun:fire') && query.is_using_item"
            },
            {
              "fire_reload": "query.is_item_name_any('slot.weapon.mainhand', 0, 'gun:fire') && (variable.attack_time > 0.0)"
            },
            {
              "mosin": "query.is_item_name_any('slot.weapon.mainhand', 0, 'gun:mosin') && query.is_using_item"
            },
            {
              "mosin_reload": "query.is_item_name_any('slot.weapon.mainhand', 0, 'gun:mosin') && (variable.attack_time > 0.0)"
            },
            {
              "ppsh": "query.is_item_name_any('slot.weapon.mainhand', 0, 'gun:ppsh') && query.is_using_item"
            },
            {
              "ppsh_reload": "query.is_item_name_any('slot.weapon.mainhand', 0, 'gun:ppsh') && (variable.attack_time > 0.0)"
            },
            {
              "dp28": "query.is_item_name_any('slot.weapon.mainhand', 0, 'gun:dp28') && query.is_using_item"
            },
            {
              "dp28_reload": "query.is_item_name_any('slot.weapon.mainhand', 0, 'gun:dp28') && (variable.attack_time > 0.0)"
            },
            {
              "tt33": "query.is_item_name_any('slot.weapon.mainhand', 0, 'gun:tt33') && query.is_using_item"
            },
            {
              "tt33_reload": "query.is_item_name_any('slot.weapon.mainhand', 0, 'gun:tt33') && (variable.attack_time > 0.0)"
            },
            {
              "k98": "query.is_item_name_any('slot.weapon.mainhand', 0, 'gun:k98') && query.is_using_item"
            },
            {
              "k98_reload": "query.is_item_name_any('slot.weapon.mainhand', 0, 'gun:k98') && (variable.attack_time > 0.0)"
            },
            {
              "mp40": "query.is_item_name_any('slot.weapon.mainhand', 0, 'gun:mp40') && query.is_using_item"
            },
            {
              "mp40_reload": "query.is_item_name_any('slot.weapon.mainhand', 0, 'gun:mp40') && (variable.attack_time > 0.0)"
            },
            {
              "p90": "query.is_item_name_any('slot.weapon.mainhand', 0, 'gun:p90') && query.is_using_item"
            },
            {
              "p90_reload": "query.is_item_name_any('slot.weapon.mainhand', 0, 'gun:p90') && (variable.attack_time > 0.0)"
            },
            {
              "m870": "query.is_item_name_any('slot.weapon.mainhand', 0, 'gun:m870') && query.is_using_item"
            },
            {
              "m870_reload": "query.is_item_name_any('slot.weapon.mainhand', 0, 'gun:m870') && (variable.attack_time > 0.0)"
            }
          ]
        },
        "no_use": {
          "on_entry": [
            "/scriptevent gvcv5:gunapply"
          ],
          "transitions": [
            {
              "default": "1.0"
            }
          ]
        },
        "tank": {
          "on_entry": [
            "/execute as @s[tag=!reload] run function vmain"
          ],
          "transitions": [
            {
              "default": "(variable.attack_time <= 0.0)"
            }
          ]
        },
        "mgg_1": {
          "on_entry": [
            "/execute as @s[tag=!reload,scores={scool=0}] run function vsub"
          ],
          "transitions": [
            {
              "default": "!query.is_using_item || !query.is_riding"
            },
            {
              "mgg_2": "query.is_riding && (query.is_using_item && query.get_equipped_item_name == 'mgg')"
            }
          ]
        },
        "mgg_2": {
          "on_entry": [
            "/execute as @s[tag=!reload,scores={scool=0}] run function vsub"
          ],
          "transitions": [
            {
              "default": "!query.is_using_item || !query.is_riding"
            },
            {
              "mgg_1": "query.is_riding && (query.is_using_item && query.get_equipped_item_name == 'mgg')"
            }
          ]
        },
        "mgj_1": {
          "on_entry": [
            "/execute unless entity @e[r=4,family=heri] as @s[tag=!reload,scores={scool=0}] run function vsub"
          ],
          "transitions": [
            {
              "default": "!query.is_jumping || !query.is_riding"
            },
            {
              "mgj_2": "query.is_jumping && query.is_riding"
            }
          ]
        },
        "mgj_2": {
          "on_entry": [
            "/execute unless entity @e[r=4,family=heri] as @s[tag=!reload,scores={scool=0}] run function vsub"
          ],
          "transitions": [
            {
              "default": "!query.is_jumping || !query.is_riding"
            },
            {
              "mgj_1": "query.is_jumping && query.is_riding"
            }
          ]
        },
        "shield": {
          "on_entry": [
            "/effect @s resistance 99999 1 true"
          ],
          "on_exit": [
            "/effect @s resistance 0"
          ],
          "transitions": [
            {
              "default": "!query.is_sneaking"
            }
          ]
        },
        "fire_reload": {
          "on_entry": [
            "/scriptevent gvcv5:reload fire"
          ],
          "transitions": [
            {
              "default": "(variable.attack_time <= 0.0)"
            }
          ]
        },
        "fire": {
          "on_entry": [
            "/scriptevent gvcv5:gunUse fire"
          ],
          "transitions": [
            {
              "no_use": "!query.is_using_item"
            },
            {
              "fireii": "query.get_equipped_item_name == 'fire' && query.is_using_item"
            }
          ]
        },
        "fireii": {
          "on_entry": [
            "/scriptevent gvcv5:gunUse fire"
          ],
          "transitions": [
            {
              "no_use": "!query.is_using_item"
            },
            {
              "fire": "query.get_equipped_item_name == 'fire' && query.is_using_item"
            }
          ]
        },
        "nouse": {
          "transitions": [
            {
              "default": "!query.is_using_item"
            }
          ]
        },
        "mosin_reload": {
          "on_entry": [
            "/execute as @s[tag=!reload,tag=!down] run scriptevent gvcv5:reload mosin"
          ],
          "transitions": [
            {
              "default": "(variable.attack_time <= 0.0)"
            }
          ]
        },
        "mosin": {
          "on_entry": [
            "/scriptevent gvcv5:gunUse mosin"
          ],
          "transitions": [
            {
              "mosin_after": "(!query.is_using_item)"
            }
          ]
        },
        "mosin_after": {
          "on_entry": [
            "/scriptevent gvcv5:gunapply"
          ],
          "transitions": [
            {
              "default": "variable.cooltime = (variable.cooltime ?? 0);variable.cooltime = variable.cooltime < 29 ? variable.cooltime + 1:0;return variable.cooltime == 0;"
            }
          ]
        },
        "ppsh_reload": {
          "on_entry": [
            "/execute as @s[tag=!reload,tag=!down] run scriptevent gvcv5:reload ppsh"
          ],
          "transitions": [
            {
              "default": "(variable.attack_time <= 0.0)"
            }
          ]
        },
        "ppsh": {
          "on_entry": [
            "/scriptevent gvcv5:gunUse ppsh"
          ],
          "transitions": [
            {
              "ppshii": "variable.cooltime = (variable.cooltime ?? 0);variable.cooltime = variable.cooltime < 1 ? variable.cooltime + 1:0;return ( (query.is_using_item) && variable.cooltime == 0);"
            },
            {
              "no_use": "(!query.is_using_item)"
            }
          ]
        },
        "ppshii": {
          "on_entry": [
            "/scriptevent gvcv5:gunUse ppsh"
          ],
          "transitions": [
            {
              "ppsh": "variable.cooltime = (variable.cooltime ?? 0);variable.cooltime = variable.cooltime < 1 ? variable.cooltime + 1:0;return ( (query.is_using_item) && variable.cooltime == 0);"
            },
            {
              "no_use": "(!query.is_using_item)"
            }
          ]
        },
        "dp28_reload": {
          "on_entry": [
            "/execute as @s[tag=!reload,tag=!down] run scriptevent gvcv5:reload dp28"
          ],
          "transitions": [
            {
