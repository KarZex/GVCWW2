            "/scriptevent gvcv5:gunUse mp40"
          ],
          "transitions": [
            {
              "mp40ii": "variable.cooltime = (variable.cooltime ?? 0);variable.cooltime = variable.cooltime < 1 ? variable.cooltime + 1:0;return ( (query.is_using_item) && variable.cooltime == 0);"
            },
            {
              "no_use": "(!query.is_using_item)"
            }
          ]
        },
        "mp40ii": {
          "on_entry": [
            "/scriptevent gvcv5:gunUse mp40"
          ],
          "transitions": [
            {
              "mp40": "variable.cooltime = (variable.cooltime ?? 0);variable.cooltime = variable.cooltime < 1 ? variable.cooltime + 1:0;return ( (query.is_using_item) && variable.cooltime == 0);"
            },
            {
              "no_use": "(!query.is_using_item)"
            }
          ]
        },
        "p90_reload": {
          "on_entry": [
            "/execute as @s[tag=!reload,tag=!down] run scriptevent gvcv5:reload p90"
          ],
          "transitions": [
